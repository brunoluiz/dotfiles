# Include /Users/brunoluiz/.colima/ssh_config
Include ~/.colima/ssh_config

Host *
  IdentityFile ~/.ssh/id_ed25519_personal
  ForwardAgent yes
  AddKeysToAgent yes
  UseKeychain yes

Host github.com
  HostName github.com
  IdentityFile ~/.ssh/id_ed25519_personal
  IdentitiesOnly yes
  AddKeysToAgent yes
  UseKeychain yes
